		{
			"name": "Angel One SmartAPI",
			"item": [
				{
					"name": "Authentication",
			"item": [
				{
					"name": "Generate Session (Login by MPIN)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.status && jsonData.data) {",
									"        pm.environment.set(\"access_token\", jsonData.data.jwtToken);",
									"        pm.environment.set(\"refresh_token\", jsonData.data.refreshToken);",
									"        pm.environment.set(\"feed_token\", jsonData.data.feedToken);",
									"    }",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Accept",
								"value": "application/json"
							},
							{
								"key": "X-UserType",
								"value": "USER"
							},
							{
								"key": "X-SourceID",
								"value": "WEB"
							},
							{
								"key": "X-ClientLocalIP",
								"value": "192.168.1.1",
								"disabled": true
							},
							{
								"key": "X-ClientPublicIP",
								"value": "192.168.1.1",
								"disabled": true
							},
							{
								"key": "X-MACAddress",
								"value": "00:00:00:00:00:00"
							},
							{
								"key": "X-PrivateKey",
								"value": "{{api_key}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"clientcode\": \"{{client_id}}\",\n    \"mpin\": \"{{mpin}}\",\n    \"totp\": \"{{totp}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/rest/auth/angelbroking/user/v1/loginByMPIN",
							"host": [
								"{{base_url}}"
